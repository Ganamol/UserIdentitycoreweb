@model IdentitycrudMVC.Models.PhotoModel



@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Product</h4>
<hr />






 < div class="row">
<div class="col-md-4">

    <form asp-action="Createpost" enctype="multipart/form-data" asp-controller="Photo">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

      

        <div class="form-group">
            <label asp-for="FilePath" class="control-label"></label>
            <input  type="file" asp-for="Fileobj" class="form-control" multiple/>
      
        </div><br />

        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </form>
</div>
 </div>


    @section Scripts {
        @{
            await Html.RenderPartialAsync("_ValidationScriptsPartial");
        }
    }
